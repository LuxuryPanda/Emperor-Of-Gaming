            <form class="form" id="postCreation" action="/v1/post" method="POST">
              <input type="hidden" name="_csrf" value="<%= csrfToken %>">
              <div class="form-row">
                <div class="form-item">
                  <div class="form-textarea">
                    <textarea id="postText" name="postText" placeholder="Ciao <%= user.name %>! Convidivi i tuoi pensieri..."></textarea>
