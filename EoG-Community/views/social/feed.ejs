  <%- include('../includes/header.ejs') %>
  <%- include('../includes/floaty-bar.ejs'); %>

  <!-- BODY -->
  <div class="content-grid">
    <!-- BANNER -->
    <div class="section-banner">
      <img class="section-banner-icon" src="img/banner/newsfeed-icon.png" alt="newsfeed-icon">
  
      <p class="section-banner-title">Newsfeed</p>
      <p class="section-banner-text">Controlla cosa hanno fatto i tuoi amici!</p>
    </div>
    <!-- FINE BANNER -->


    <div class="grid grid-3-6-3 mobile-prefer-content">
      <!-- COLONNA SINISTRA -->
