              <!-- OPZIONE STATO -->
              <div class="option-item active">
                <!-- ICONA -->
                <svg class="option-item-icon icon-status">
                  <use xlink:href="#svg-status"></use>
                </svg>
                <!-- FINE ICONA -->
      
                <p class="option-item-title">Stato</p>
              </div>
              <!-- FINE STATO -->
      
              <!-- OPZIONE POST BLOG -->
              <div class="option-item">
                <!-- ICONA -->
                <svg class="option-item-icon icon-blog-posts">
                  <use xlink:href="#svg-blog-posts"></use>
                </svg>
                <!-- FINE ICONA -->
      
                <p class="option-item-title">Post Blog</p>
              </div>
              <!-- FINE BLOG -->
      
              <!-- OPZIONE SONDAGGIO -->
              <div class="option-item">
                <!-- ICONA -->
                <svg class="option-item-icon icon-poll">
                  <use xlink:href="#svg-poll"></use>
                </svg>
                <!-- FINE ICONA -->

                <p class="option-item-title">Sondaggio</p>
              </div>
              <!-- FINE SONDAGGIO -->
            </div>
          </div>
