            <!-- PROGRESSO -->
            <div class="progress-arc-wrap">
              <div class="progress-arc">
                <canvas id="profile-completion-chart"></canvas>
              </div>
        
              <div class="progress-arc-info">
                <p class="progress-arc-title">70%</p>
              </div>
            </div>
            <!-- FINE PROGRESSO -->
        
